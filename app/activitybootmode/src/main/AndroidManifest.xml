<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.njfu.activitybootmode">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activilitylife.MainActivity3"></activity>
        <activity android:name=".activilitylife.BaseActivity" />
        <activity android:name=".activilitylife.ActivityController" />
        <activity android:name=".activilitylife.Singleinstance" />
        <activity android:name=".activilitylife.SingleTop" />
        <activity android:name=".activilitylife.SingleTask" />
        <activity android:name=".activilitylife.MainActivity2" />
        <activity android:name=".activilitylife.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>